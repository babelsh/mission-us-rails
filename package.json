    "babel-jest": "^23.4.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "jest": "^23.5.0",
    "jest-vue": "^0.8.2",
    "jest-vue-preprocessor": "^1.4.0",
    "material-design-icons-iconfont": "^3.0.3",
    "vue": "^2.5.17",
    "vue-loader": "14.2.2",
    "vue-template-compiler": "^2.5.17",
    "vuetify": "1.1.0"
  },
  "devDependencies": {
    "webpack-dev-server": "2.11.2"
